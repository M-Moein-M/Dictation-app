<div class="container mt-5">
  {{#each rankings}}
  {{#if this}}
  <div class="row justify-content-center">
    <div class="col-sm-10 col-md-6 ranking mt-3">
      <div class="float-left ranking-username">
        {{this.username}}
      </div>
      <div class="float-right ranking-score">
        {{this.score}}
      </div>
    </div>
  </div>
  {{/if}}
  {{/each}}

</div>